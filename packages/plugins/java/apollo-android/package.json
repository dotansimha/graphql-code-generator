{
  "name": "@graphql-codegen/java-apollo-android",
  "version": "1.8.1",
  "description": "GraphQL Code Generator plugin for generating Java classes for Apollo-Android",
  "repository": "git@github.com:dotansimha/graphql-code-generator.git",
  "license": "MIT",
  "scripts": {
    "test": "jest --config ../../../../jest.config.js",
    "build": "tsc -m esnext --outDir dist/esnext && tsc -m commonjs --outDir dist/commonjs"
  },
  "dependencies": {
    "@graphql-codegen/java-common": "1.8.1",
    "@graphql-codegen/plugin-helpers": "1.8.1",
    "@graphql-codegen/visitor-plugin-common": "1.8.1",
    "pluralize": "8.0.0",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@types/pluralize": "0.0.29",
    "graphql": "14.5.8",
    "java-ast": "0.1.0",
    "jest": "24.8.0",
    "ts-jest": "24.1.0",
    "typescript": "3.6.4"
  },
  "sideEffects": false,
  "main": "dist/commonjs/index.js",
  "module": "dist/esnext/index.js",
  "typings": "dist/esnext/index.d.ts",
  "typescript": {
    "definition": "dist/esnext/index.d.ts"
  },
  "publishConfig": {
    "access": "public"
  }
