{
  "extends": "../../../../tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "importHelpers": true,
    "module": "esnext",
    "target": "es2018",
    "lib": ["es6", "esnext", "es2015"],
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "outDir": "./dist/",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["src"]
}
