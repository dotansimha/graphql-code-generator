{
  "name": "@graphql-codegen/typescript-mongodb",
  "version": "1.9.0",
  "description": "GraphQL Code Generator plugin for generting a ready-to-use ORM types for MongoDB",
  "repository": "git@github.com:dotansimha/graphql-code-generator.git",
  "license": "MIT",
  "scripts": {
    "prepack": "bob-update-version",
    "build": "bob",
    "test": "jest --no-watchman --config ../../../../jest.config.js"
  },
  "peerDependencies": {
    "graphql-tag": "^2.0.0"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "1.9.0",
    "@graphql-codegen/typescript": "1.9.0",
    "@graphql-codegen/visitor-plugin-common": "1.9.0",
    "@graphql-toolkit/common": "0.7.3",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@ardatan/bob": "0.1.6",
    "@graphql-codegen/testing": "1.9.0",
    "graphql": "14.5.8",
    "jest": "24.9.0",
    "ts-jest": "24.2.0",
    "typescript": "3.7.2"
  },
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
