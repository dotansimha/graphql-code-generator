  Node: ResolversTypes['SomeNode'];
  ID: ResolverTypeWrapper<Scalars['ID']>;
  SomeNode: ResolverTypeWrapper<SomeNode>;
  MyUnion: ResolversTypes['MyType'] | ResolversTypes['MyOtherType'];
