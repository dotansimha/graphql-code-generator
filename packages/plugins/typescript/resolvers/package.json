{
  "name": "@graphql-codegen/typescript-resolvers",
  "version": "1.8.3",
  "description": "GraphQL Code Generator plugin for generating TypeScript types for resolvers signature",
  "repository": "git@github.com:dotansimha/graphql-code-generator.git",
  "license": "MIT",
  "scripts": {
    "build": "tsc -m esnext --outDir dist/esnext && tsc -m commonjs --outDir dist/commonjs",
    "test": "jest --no-watchman --config ../../../../jest.config.js"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "1.8.3",
    "@graphql-codegen/typescript": "1.8.3",
    "@graphql-codegen/visitor-plugin-common": "1.8.3",
    "@graphql-toolkit/common": "0.7.0",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@graphql-codegen/testing": "1.8.3",
    "graphql": "14.5.8",
    "graphql-subscriptions": "1.1.0",
    "graphql-tools": "4.0.6",
    "jest": "24.9.0",
    "ts-jest": "24.1.0",
    "typescript": "3.7.2"
  },
  "sideEffects": false,
  "main": "dist/commonjs/index.js",
  "module": "dist/esnext/index.js",
  "typings": "dist/esnext/index.d.ts",
  "typescript": {
    "definition": "dist/esnext/index.d.ts"
  },
  "jest-junit": {
    "outputDirectory": "../../../../test-results/typescript-resolvers"
  },
  "publishConfig": {
    "access": "public"
  }
}
