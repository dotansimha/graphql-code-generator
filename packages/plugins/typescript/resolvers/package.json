{
  "name": "@graphql-codegen/typescript-resolvers",
  "version": "1.6.1",
  "description": "GraphQL Code Generator plugin for generating TypeScript types for resolvers signature",
  "repository": "git@github.com:dotansimha/graphql-code-generator.git",
  "license": "MIT",
  "scripts": {
    "build": "tsc -m esnext --outDir dist/esnext && tsc -m commonjs --outDir dist/commonjs",
    "test": "jest --config ../../../../jest.config.js"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "1.6.1",
    "@graphql-codegen/typescript": "1.6.1",
    "@graphql-codegen/visitor-plugin-common": "1.6.1",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@graphql-codegen/testing": "1.6.1",
    "graphql": "14.4.2",
    "graphql-subscriptions": "1.1.0",
    "graphql-tools": "4.0.5",
    "jest": "24.9.0",
    "ts-jest": "24.0.2",
    "typescript": "3.5.3"
  },
  "sideEffects": false,
  "main": "dist/commonjs/index.js",
  "module": "dist/esnext/index.js",
  "typings": "dist/esnext/index.d.ts",
  "typescript": {
    "definition": "dist/esnext/index.d.ts"
  },
  "jest-junit": {
    "outputDirectory": "../../../../test-results/typescript-resolvers"
  },
  "publishConfig": {
    "access": "public"
  }
}
