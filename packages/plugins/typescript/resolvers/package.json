    "@graphql-codegen/typescript": "1.11.2",
    "@graphql-codegen/visitor-plugin-common": "1.11.2",
    "@graphql-toolkit/common": "0.9.1",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@graphql-codegen/testing": "1.11.2",
    "bob-the-bundler": "0.2.2",
    "graphql": "14.5.8",
    "graphql-subscriptions": "1.1.0",
    "graphql-tools": "4.0.6",
    "jest": "24.9.0",
    "ts-jest": "24.3.0",
    "typescript": "3.7.4"
  },
  "sideEffects": false,
  "main": "dist/index.cjs.js",
