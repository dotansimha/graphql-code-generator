  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "1.8.3",
    "@graphql-toolkit/common": "0.6.6",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "jest": "24.9.0",
    "ts-jest": "24.1.0",
    "tslib": "1.10.0",
    "typescript": "3.7.2"
  },
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "sideEffects": false,
  "main": "dist/commonjs/index.js",
