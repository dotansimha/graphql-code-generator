    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@graphql-codegen/testing": "1.2.0",
    "graphql": "14.3.1",
    "jest": "24.8.0",
    "ts-jest": "24.0.2",
    "typescript": "3.4.5"
  },
  "sideEffects": false,
  "main": "dist/commonjs/index.js",
  "module": "dist/esnext/index.js",
  "typings": "dist/esnext/index.d.ts",
