{
  "name": "@graphql-codegen/client-preset",
  "version": "4.0.0",
  "description": "GraphQL Code Generator preset for client.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-code-generator.git",
    "directory": "packages/presets/client"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint **/*.ts",
    "test": "jest --no-watchman --config ../../../jest.config.js"
  },
  "devDependencies": {
    "@babel/types": "^7.18.13",
    "@types/babel__core": "^7.20.0",
    "@types/babel__helper-plugin-utils": "7.10.0",
    "@types/babel__template": "7.4.1",
    "@types/babel__traverse": "^7.18.5",
    "jiti": "^1.17.1"
  },
  "dependencies": {
    "@babel/core": "7.21.8",
    "@babel/helper-plugin-utils": "^7.20.2",
    "@babel/template": "^7.20.7",
    "@babel/traverse": "^7.21.4",
    "@graphql-codegen/add": "workspace:*",
    "@graphql-codegen/gql-tag-operations": "workspace:*",
    "@graphql-codegen/plugin-helpers": "workspace:*",
    "@graphql-codegen/typed-document-node": "workspace:*",
    "@graphql-codegen/typescript": "workspace:*",
    "@graphql-codegen/typescript-operations": "workspace:*",
    "@graphql-codegen/visitor-plugin-common": "workspace:*",
    "@graphql-tools/documents": "^1.0.0",
    "@graphql-tools/utils": "^10.0.0",
    "@graphql-typed-document-node/core": "3.2.0",
    "tslib": "~2.5.0"
  },
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "type": "module"
}
