{
  "name": "my-codegen-template",
  "version": "0.0.1",
  "description": "",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "codegen-templates-scripts build && tsc -p tsconfig.es2015.json",
    "pretest": "yarn build",
    "test": "codegen-templates-scripts test"
  },
  "devDependencies": {},
  "main": "./dist/index.js",
  "module": "./dist/es2015/index.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": false
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
