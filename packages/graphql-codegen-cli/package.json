{
  "name": "graphql-code-generator",
  "version": "0.13.0",
  "license": "MIT",
  "bin": {
    "gql-gen": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotansimha/graphql-codegen.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "tslint src/**/*.ts",
    "prebuild": "yarn clean && yarn lint",
    "build": "tsc",
    "prestart": "yarn build",
    "start": "cd dist && node index.js",
    "debug": "cd dist && node --inspect --debug-brk index.js",
    "test": "jest --no-cache --verbose --runInBand"
  },
  "keywords": [
    "gql",
    "generator",
    "code",
    "types",
    "interfaces",
    "graphql",
    "codegen",
    "apollo",
    "node",
    "typescript",
    "ts",
    "flow",
    "types",
    "d.ts",
    "typings"
  ],
  "author": "Dotan Simha <dotansimha@gmail.com>",
  "bugs": {
    "url": "https://github.com/dotansimha/graphql-codegen/issues"
  },
  "homepage": "https://github.com/dotansimha/graphql-codegen#readme",
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./tests/coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".*?handlebars-helpers.*?",
      "src/index.ts",
      "src/types.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/tests/jest.typescript.js"
    },
    "testRegex": "tests/.*\\.spec.ts$",
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "dependencies": {
    "@graphql-modules/epoxy": "0.2.1",
    "@types/babylon": "6.16.4",
    "@types/is-glob": "4.0.0",
    "@types/prettier": "1.13.2",
    "@types/valid-url": "1.0.2",
    "babel-types": "7.0.0-beta.3",
    "babylon": "7.0.0-beta.47",
    "chalk": "2.4.1",
    "commander": "2.19.0",
    "fb-watchman": "2.0.0",
    "glob": "7.1.3",
    "graphql-codegen-compiler": "0.13.0",
    "graphql-codegen-core": "0.13.0",
    "graphql-config": "2.2.1",
    "graphql-import": "0.7.1",
    "is-glob": "4.0.0",
    "is-valid-path": "0.1.1",
    "mkdirp": "0.5.1",
    "ora": "3.0.0",
    "pify": "4.0.1",
    "prettier": "1.15.2",
    "request": "2.88.0",
    "valid-url": "1.0.9"
  },
  "devDependencies": {
    "@types/chalk": "2.2.0",
    "@types/fb-watchman": "2.0.0",
    "@types/ora": "1.3.4",
    "@types/pify": "3.0.2",
    "graphql-codegen-typescript-template": "0.13.0",
    "graphql-codegen-typescript-template-multiple": "0.9.4"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "typescript": {
    "definition": "./dist/index.d.ts"
  }
}
