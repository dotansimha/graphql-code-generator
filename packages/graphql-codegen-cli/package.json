{
  "name": "graphql-code-generator",
  "version": "0.8.21",
  "license": "MIT",
  "bin": {
    "gql-gen": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotansimha/graphql-codegen.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "tslint src/**/*.ts",
    "prebuild": "yarn clean && yarn lint",
    "build": "tsc",
    "prestart": "yarn build",
    "start": "cd dist && node index.js",
    "debug": "cd dist && node --inspect --debug-brk index.js",
    "test": "jest --no-cache --verbose --runInBand"
  },
  "keywords": [
    "gql",
    "generator",
    "code",
    "types",
    "interfaces",
    "graphql",
    "codegen",
    "apollo",
    "node",
    "typescript",
    "ts",
    "flow",
    "types",
    "d.ts",
    "typings"
  ],
  "author": "Dotan Simha <dotansimha@gmail.com>",
  "bugs": {
    "url": "https://github.com/dotansimha/graphql-codegen/issues"
  },
  "homepage": "https://github.com/dotansimha/graphql-codegen#readme",
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": ["src/**/*.ts"],
    "coverageDirectory": "./tests/coverage",
    "coveragePathIgnorePatterns": ["/node_modules/", ".*?handlebars-helpers.*?", "src/index.ts", "src/types.ts"],
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/tests/jest.typescript.js"
    },
    "testRegex": "tests/.*\\.spec.ts$",
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || || ^0.13.0"
  },
  "dependencies": {
    "@types/prettier": "1.12.0",
    "commander": "2.15.1",
    "glob": "7.1.2",
    "graphql-codegen-compiler": "0.8.21",
    "graphql-codegen-core": "0.8.21",
    "graphql-codegen-generators": "0.8.21",
    "mkdirp": "0.5.1",
    "prettier": "1.12.1",
    "request": "2.85.0",
    "strip-comments": "1.0.1"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  }
}
