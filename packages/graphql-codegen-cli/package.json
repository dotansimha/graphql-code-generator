    "@graphql-tools/url-loader": "^7.12.1",
    "@graphql-tools/utils": "^8.8.0",
    "ansi-escapes": "^4.3.1",
    "chalk": "^4.1.0",
    "chokidar": "^3.5.2",
    "cosmiconfig": "^7.0.0",
    "cross-undici-fetch": "^0.4.11",
    "debounce": "^1.2.0",
    "detect-indent": "^6.0.0",
    "graphql-config": "^4.3.1",
    "inquirer": "^8.0.0",
    "is-glob": "^4.0.1",
    "json-to-pretty-yaml": "^1.2.2",
