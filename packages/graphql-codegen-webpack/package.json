{
  "name": "graphql-codegen-webpack",
  "version": "0.13.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotansimha/graphql-codegen.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "tslint src/**/*.ts",
    "prebuild": "yarn clean && yarn lint",
    "build": "tsc",
    "prestart": "yarn build",
    "start": "cd dist && node index.js",
    "debug": "cd dist && node --inspect --debug-brk index.js",
    "test": "jest --no-cache --verbose --runInBand"
  },
  "keywords": [
    "gql",
    "generator",
    "code",
    "types",
    "interfaces",
    "graphql",
    "codegen",
    "apollo",
    "node",
    "typescript",
    "ts",
    "flow",
    "types",
    "d.ts",
    "typings",
    "webpack"
  ],
  "author": "Kamil Kisiela <kamil.kisiela@gmail.com>",
  "bugs": {
    "url": "https://github.com/dotansimha/graphql-codegen/issues"
  },
  "homepage": "https://github.com/dotansimha/graphql-codegen#readme",
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./tests/coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/tests/jest.typescript.js"
    },
    "testRegex": "tests/.*\\.spec.ts$",
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0",
    "webpack": "^4.0.0"
  },
  "dependencies": {
    "graphql-code-generator": "0.13.0"
  },
  "devDependencies": {
    "@types/webpack": "4.4.19"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "typescript": {
    "definition": "./dist/index.d.ts"
  }
}
